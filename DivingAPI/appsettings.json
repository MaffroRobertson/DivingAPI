{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Jwt": {
    "Issuer": "DivingAPI",
    "Audience": "DivingAPIUsers"
  },
  "Auth": {
    "RefreshTokens": {
      "MaxActivePerUser": 5,
      "CleanupIntervalHours": 6
    }
  },
  "AllowedHosts": "*",
